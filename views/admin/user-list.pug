doctype html
html(lang="en")
  include ../includes/head.pug
  body
    include ../includes/navbar.pug
    main
      if userList.length > 0
        table
          tr
            th Username
            th Email
          each user in userList
            tr
              td= user.username
              td= user.email
              //- td
              //-   a(href=('/admin/manage-book/edit/' + book._id)) Edit
              //-   a(href=('/admin/manage-book/delete/' + book._id)) Delete
      else
        h2 Users not found
    include ../includes/footer.pug